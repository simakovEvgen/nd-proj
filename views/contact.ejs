<%- include('layout/header') -%>
<div class="container green-row">
    <div class="max-1200">
        <div class="place">
            <h1 class="whereami">GOT A QUESTION OR INQUIRY</h1>
        </div>
    </div>
</div>
<div class="map">
    <iframe src="https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d492486.7731279493!2d73.73194118744418!3d15.34651318053335!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x3bbfba106336b741%3A0xeaf887ff62f34092!2z0pDQvtCwLCDQhtC90LTRltGP!5e0!3m2!1suk!2sua!4v1502796629611" frameborder="0" allowfullscreen></iframe>
</div>
<div class="container max-1200">
    <h1 class="contact-header">CONTACT FORM</h1>
    <form id="formMessage" class="contact-form">
        <input id="name" class="name" placeholder="Name" type="text" name="text" required autocomplete="off" />
        <input id="email" class="email" placeholder="Email" type="email" name="email" required autocomplete="off" />
        <input id="subject" class="subject" placeholder="Subject" type="text" name="subject" required autocomplete="off" />
        <textarea id="messageText" name="message" class="message" ></textarea>
        <button id="subButton" class="form-btn">SEND MESSAGE</button>
    </form>
</div>
<%- include('layout/footer') -%>